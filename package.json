{
  "name": "lipsumtext",
  "displayName": "LipsumText generate text",
  "description": "LipsumText is a powerful Visual Studio Code extension designed to streamline your development workflow by effortlessly generating placeholder paragraphs within your code. Whether you're prototyping, designing, or simply need placeholder text, LipsumText has got you covered.",
  "version": "1.1.0",
  "publisher": "learnwithyan",
  "icon": "images/logo_128.png",
  "bugs": {
    "url": "https://github.com/learnwithyan/lipsumtext",
    "email": "learnwithyan@hotmail.com"
  },
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lipsumtext.com1",
        "title": "%lipsumtext.com1.title%",
        "icon": "$(sort-precedence)"
      },
      {
        "command": "lipsumtext.com2",
        "title": "%lipsumtext.com2.title%",
        "icon": "$(sort-precedence)"
      },
      {
        "command": "lipsumtext.com3",
        "title": "%lipsumtext.com3.title%",
        "icon": "$(sort-precedence)"
      },
      {
        "command": "lipsumtext.com4",
        "title": "%lipsumtext.com4.title%",
        "icon": "$(sort-precedence)"
      },
      {
        "command": "lipsumtext.com5",
        "title": "%lipsumtext.com5.title%",
        "icon": "$(sort-precedence)"
      },
      {
        "command": "lipsumtext.com6",
        "title": "%lipsumtext.com6.title%",
        "icon": "$(sort-precedence)"
      }
    ]
  },
  "localize": [
    "package.nls.json",
    "package.bg.nls.json",
    "package.cs.nls.json",
    "package.de.nls.json",
    "package.es.nls.json",
    "package.fr.nls.json",
    "package.hu.nls.json",
    "package.it.nls.json",
    "package.ko.nls.json",
    "package.pl.nls.json",
    "package.pt.nls.json",
    "package.ru.nls.json",
    "package.tr.nls.json",
    "package.zh.nls.json"
  ],
  "configuration": {
    "title": "LipsumText Configarations",
    "properties": {
      "lipsumtext.lineBreak": {
        "type": "string",
        "default": "\n",
        "scope": "resource",
        "description": "Description of your setting"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/learnwithyan/lipsumtext.git"
  },
  "dependencies": {
    "vscode-nls": "^5.2.0"
  }
}
